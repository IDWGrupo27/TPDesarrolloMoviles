{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@shares/*": [
        "./shares/*"
      ],
      "@core/*": [
        "./core/*"
      ]
    }
  }
}